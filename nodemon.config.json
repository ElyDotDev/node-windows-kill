{
  "restartable": "rs",
  "ignoreRoot": [
    "**/.git/**",
    "**/.nyc_output/**",
    "**/.sass-cache/**",
    "**/bower_components/**",
    "**/coverage/**"
  ],
  "watch": [
    "lib/",
    "development/"
  ],
  "verbose": true,
  "ext": "js json yaml",
  "delay": "700",
  "env": {
    "NODE_ENV": "development",
    "PORT": 8080,
    "RENDERER_TYPE": "mapnik",
    "RENDERER_POOL_MIN": 1,
    "RENDERER_POOL_MAX": 4,
    "RENDERER_TILE_SIZE": 256,
    "RENDERER_MAP_TYPE_ID": "wayzy"
  }
}
