{
  "name": "windows-kill",
  "version": "0.1.0",
  "description": "Sending signals on Windows OS",
  "keywords": [
    "windows",
    "kill",
    "signal",
    "process",
    "sigbreak",
    "sigint"
  ],
  "license": "MIT",
  "homepage": "https://github.com/alirdn/node-windows-kill#readme",
  "bugs": {
    "url": "https://github.com/alirdn/node-windows-kill/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alirdn/node-windows-kill.git"
  },
  "author": {
    "name": "Alireza Dabiri Nejad / alirdn",
    "email": "alireza.dabirinejad@live.com",
    "url": "http://allii.ir/"
  },
  "contributors": [
    {
      "name": "Alireza Dabiri Nejad / alirdn",
      "email": "alireza.dabirinejad@live.com",
      "url": "http://allii.ir/"
    }
  ],
  "main": "lib/index.js",
  "gypfile": true,
  "scripts": {
    "install": "node scripts/install.js",
    "install-native": "node-pre-gyp install --fallback-to-build",
    "node-pre-gyp-package": "node-pre-gyp package",
    "node-pre-gyp-github-release": "node-pre-gyp-github publish --release",
    "test": "nyc ava",
    "test-non-windows": "nyc ava test/*.non-windows.test.js",
    "test-dev": "nyc ava --watch --verbose",
    "coverage-html": "nyc report --reporter=html",
    "coverage-codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "binary": {
    "module_name": "windows-kill",
    "module_path": "./lib/binding/",
    "host": "https://github.com/alirdn/node-windows-kill/releases/download/",
    "remote_path": "{version}"
  },
  "ava": {
    "files": [
      "test/*.test.js",
      "!test/*.non-windows.test.js"
    ]
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "nan": "^2.10.0",
    "node-pre-gyp": "=0.9.1"
  },
  "devDependencies": {
    "ava": "1.0.0-beta.3",
    "codecov": "^3.0.4",
    "istanbul": "^0.4.5",
    "node-pre-gyp-github": "^1.3.1",
    "nyc": "^12.0.2"
  }
}
